{
  "title": "Auth0 Logs to New Relic",
  "name": "auth0-logs-to-new-relic",
  "version": "1.2.0",
  "description": "This extension will take all of your Auth0 logs and export them to New Relic Logs",
  "docsUrl": "https://github.com/y-o-u/auth0-logs-to-newrelic/blob/master/README.md",
  "logoUrl": "https://newrelic.com/assets/newrelic/source/PNG/NR_logo_Horizontal.png",
  "secrets": {
    "NEWRELIC_LICENSEKEY": {
      "description": "New Relic license key",
      "required": true
    },
    "NEWRELIC_HTTP_ENDPOINT": {
      "description": "New Relic Logs Http endpoint",
      "required": true
    }
  }
}